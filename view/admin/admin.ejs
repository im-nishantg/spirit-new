<!-- header -->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">

    <link rel="stylesheet" href="/css/all.css">
    <!-- navbar -->
</head>

<body>

    <nav class=" navbar navbar-dark navbar-expand-lg bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#"><i class="fa-solid fa-screwdriver-wrench"></i> Admin</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a href="/admin/admin_home/events" class="nav-link active">Events</a>
                    </li>
                    <li class="nav-item">
                        <a href="/admin/admin_home" class="nav-link active">Registrations</a>
                    </li>
                  
                    <li class="nav-item">
                        <a href="/admin/logout" class="btn btn-sm btn-danger float-end">log out</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <!-- drop your code here ðŸ‘‡ðŸ‘‡ðŸ‘‡ðŸ‘‡-->


    <div class="container">
        <div class="row data-box">
            <div class="data-table table-responsive  col-sm-12 mt-4">
                <h3 class="head-text text-center alert alert-success">Registered client</h3>
                <table class="table table-bordered border-dark table-striped table-hover">
                    <thead>
                        <tr>
                            <th class="_head" scope="col">#</th>
                            <th class="" scope="col" style="background:#3D4D5A; color:white;">Name</th>
                            <th class="" scope="col" style="background:#795548; color:white;">Email</th>
                            <th class="" scope="col" style="background:rgb(33, 190, 252); color:white;">Phone</th>
                            <th class="" scope="col" style="background:slategrey; color:white;">Whatsapp number</th>
                            <th class="" scope="col" style="background:teal; color:white;">College name</th>
                            <th class="" scope="col" style="background:brown; color:white;">year</th>
                            <th class="" scope="col" style="background: green; color:white;">Payment status</th>
                        </tr>
                    </thead>

                    <tbody class="table-group-divider">
                        <% var i=1 %>
                            <% data.forEach((item)=>{ %>

                                <tr>
                                    <th scope="row">
                                        <%= i++ %>
                                    </th>
                                    <td class="" style="color: white; background:#3D4D5A; font-weight:600">
                                        <%= item.name %>
                                    </td>
                                    <td style=" font-weight:600">
                                        <%= item.email %>
                                    </td>
                                    <td>
                                        <%=item.phone %>
                                    </td>
                                    <td style="color: black; font-weight:800">
                                        <%=item.whatsapp %>
                                    </td>
                                    <td style=" font-weight:600">
                                        <%=item.college %>
                                    </td>
                                    <td>
                                        <%=item.year %>
                                    </td>
                                    <td style="color: green;font-weight:600">
                                        <%=item.payment_status %>
                                    </td>

                                </tr>
                                <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>


    <style>
        .data-table {

            padding: 5px 4px;
            margin: 1px;
            box-shadow: rgba(0, 0, 0, 0.16) 0px 3px 6px, rgba(0, 0, 0, 0.23) 0px 3px 6px;
        }
    </style>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
    crossorigin="anonymous"></script>

    <script src="/js/all.js"></script>
</body>

</html>